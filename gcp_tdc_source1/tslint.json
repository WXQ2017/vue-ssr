{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-react"],
  "jsRules": {},
  "rules": {
    "indent": [true, "spaces", 2],
    "member-access": false,
    "arrow-parens": false,
    "variable-name": false,
    // 这个规则强制对象中的 key 按照字母顺序排列 
    "object-literal-sort-keys": false,
    "jsx-wrap-multiline": false
  },
  "rulesDirectory": []
}
